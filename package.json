{
  "name": "v-table-component",
  "version": "0.0.15",
  "type": "module",
  "private": false,
  "files": [
    "dist",
    "types"
  ],
  "main": "./dist/v-table-component.umd.js",
  "module": "./dist/v-table-component.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/v-table-component.es.js",
      "require": "./dist/v-table-component.umd.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly && mv dist/src dist/types",
    "types": "vue-tsc --declaration",
    "preview": "vite preview"
  },
  "dependencies": {
    "@suadelabs/vue3-multiselect": "^1.0.2",
    "vue": "^3.4.37"
  },
  "devDependencies": {
    "@types/node": "^22.5.1",
    "@vitejs/plugin-vue": "^5.1.2",
    "autoprefixer": "^10.4.17",
    "tailwindcss": "^3.4.1",
    "tailwindcss-rtl": "^0.9.0",
    "typescript": "^5.5.3",
    "vite": "^5.4.1",
    "vite-plugin-css-injected-by-js": "^3.5.1",
    "vite-plugin-dts": "^4.0.3",
    "vue-tsc": "^2.0.29"
  }
}
